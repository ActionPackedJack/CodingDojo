@model UserDashboard.Models.RegisterVM

@{
    if(TempData["error"] != null){
        <h1>@Tempdata["error "]</h1>
    }
}

<form asp-controller="UserDashboard" asp-action="registerPost">
    <p>Email: <input asp-for="email"></p>
    <span asp-validation-for="email"></span>

    <p>First Name: <input asp-for="first"></p>
    <span asp-validation-for="first"></span>

    <p>Last Name: <input asp-for="last"></p>
    <span asp-validation-for="last"></span>

    <p>Password: <input asp-for="password"></p>
    <span asp-validation-for="password"></span>

    <p>Confirm Password: <input asp-for="confirm"></p>
    <span asp-validation-for="confirm "></span>
</form>